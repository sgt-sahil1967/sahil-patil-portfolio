{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "pythonVersion": "3.9" }
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "static/$1" },
    { "src": "/(.*)",        "dest": "main.py" }
  ],
  // replicate your UI “Install Command” here:
  "installCommand": "pip install -r requirements.txt",
  // if you had a “Build Command” in the UI, put it here:
  "buildCommand": "",             
  // and same for Dev:
  "devCommand": "vercel dev"
}
